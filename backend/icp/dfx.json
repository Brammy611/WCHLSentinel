{
    "version": 1,
    "canisters": {
    "face_recognition": {
      "type": "custom",
      "candid": "src/face_recognition/face_recognition.did",
      "wasm": "target/wasm32-wasip1/release/face_recognition.wasm",
      "build": "cargo build --target wasm32-wasip1 --release"
    }
    },
    "defaults": {
    "build": {
      "output": "wasm"
    }
    },
    "output_env_file": ".env",
    "networks": {
      "local": {
        "bind": "127.0.0.1:4943",
        "type": "ephemeral"
      },
      "ic": {
        "providers": ["https://ic0.app"],
        "type": "persistent"
      }
    }
  }
